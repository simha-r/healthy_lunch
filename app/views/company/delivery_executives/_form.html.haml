= form_for  [:company,@delivery_executive],:html => { :multipart => true } do |f|
  - if @delivery_executive.errors.any?
    #error_explanation
      %h2= "#{pluralize(@delivery_executive.errors.count, "error")} prohibited this product from being saved:"
      %ul
        - @delivery_executive.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :name
    = f.text_field :name
  .field
    = f.label :phone_number
    = f.text_field :phone_number
  .field
    = f.label :email
    = f.text_field :email
  .actions
    = f.submit