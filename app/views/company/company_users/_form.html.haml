= form_for  [:company,@company_user],:html => { :multipart => true } do |f|
  - if @company_user.errors.any?
    #error_explanation
      %h2= "#{pluralize(@company_user.errors.count, "error")} prohibited this product from being saved:"
      %ul
        - @company_user.errors.full_messages.each do |msg|
          %li= msg

  .field
    = f.label :email
    = f.text_field :email
  .field
    = f.label :password
    = f.text_field :password
  .field
    = f.label :password_confirmation
    = f.text_field :password_confirmation
  .actions
    = f.submit
  %br